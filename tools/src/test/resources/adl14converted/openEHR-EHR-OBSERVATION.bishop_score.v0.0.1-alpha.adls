                description = <"Diameter of the cervical os. (synthesised)">
            >
            ["ac9004"] = <
                text = <"Fetal station (synthesised)">
                description = <"The position of the fetal head in relation to the ischial spines. (synthesised)">
            >
            ["id32"] = <
                text = <"Extension">
