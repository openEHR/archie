                                    use_node ELEMENT[id9024] occurrences matches {1} /data[id2]/items[id74]/items[id105]/items[id106]
                                    use_node ELEMENT[id9025] occurrences matches {1} /data[id2]/items[id74]/items[id105]/items[id107]
                                    use_node ELEMENT[id9026] occurrences matches {1} /data[id2]/items[id74]/items[id105]/items[id108]
                                }
