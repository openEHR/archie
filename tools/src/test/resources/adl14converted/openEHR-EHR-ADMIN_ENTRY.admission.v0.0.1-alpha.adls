                            }
                            ELEMENT[id101] occurrences matches {0..1} matches {    -- Family name
                                value matches {
                                    DV_TEXT[id9040] 
                                }
                            }
                            ELEMENT[id121] occurrences matches {0..1} matches {    -- Last name
                                value matches {
                                    DV_TEXT[id9041] 
                                }
                            }
                        }
                    }
                    CLUSTER[id26] occurrences matches {0..1} matches {    -- Referring doctor
                        items cardinality matches {1..*; unordered} matches {
                            use_node ELEMENT[id9042] occurrences matches {1} /data[id2]/items[id99]/items[id100]
                            use_node ELEMENT[id9043] occurrences matches {1} /data[id2]/items[id99]/items[id101]
                            use_node ELEMENT[id9044] occurrences matches {1} /data[id2]/items[id99]/items[id121]
                        }
                    }
                    CLUSTER[id122] matches {    -- Consulting doctor
                        items cardinality matches {1..*; unordered} matches {
                            use_node ELEMENT[id9045] occurrences matches {1} /data[id2]/items[id99]/items[id100]
