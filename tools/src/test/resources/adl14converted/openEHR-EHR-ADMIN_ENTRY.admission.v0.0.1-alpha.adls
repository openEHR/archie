                            use_node ELEMENT[id9038] occurrences matches {1} /data[id2]/items[id74]/items[id104]
                        }
                    }
                    CLUSTER[id99] matches {    -- Attending doctor
                        items cardinality matches {1..*; unordered} matches {
                            ELEMENT[id100] occurrences matches {0..1} matches {    -- ID
                                value matches {
