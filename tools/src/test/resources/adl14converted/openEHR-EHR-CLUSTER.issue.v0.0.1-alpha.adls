definition
    CLUSTER[id1] matches {    -- Issue presented by a person
        items cardinality matches {1..*; unordered} matches {
            ELEMENT[id2] occurrences matches {1} matches {    -- Issue
                value matches {
                    DV_TEXT[id9000] 
                }
            }
            ELEMENT[id3] occurrences matches {0..1} matches {    -- Detail
                value matches {
                    DV_TEXT[id9001] 
                }
            }
        }
    }

