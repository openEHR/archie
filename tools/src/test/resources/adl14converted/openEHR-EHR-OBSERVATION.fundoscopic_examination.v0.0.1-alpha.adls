                            DV_BOOLEAN[id9029] matches {
                                value matches {True, False; False}
                            }
                        }
                    }
                    ELEMENT[id60] occurrences matches {0..1} matches {    -- Quality of visualisation
                        value matches {
                            DV_ORDINAL[id9030] matches {
