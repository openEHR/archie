                    }
                    ELEMENT[id34] occurrences matches {0..1} matches {    -- Measurement endpoint
                        value matches {
                            DV_TEXT[id9022] 
                        }
                    }
                    allow_archetype CLUSTER[id36] matches {     -- Structured endpoint
