            >
            ["id73"] = <
                text = <"*Pack definition(en)">
                description = <"*The definition of the size of pack used as part of the algorithm for calculating 'Pack years' data elements used in this archetype.(en)">
                comment = <"*Definition may need to be specified per type. For example, the number of units in a pack used for cigarettes may be different to cigars; the mass of loose tobacco in a pack used for hand-rolled cigarettes or pipes.(en)">
            >
            ["id72"] = <
                text = <"*Quit date definition(en)">
