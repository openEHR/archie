definition
    CLUSTER[id1] matches {    -- Tumour - Colorectal staging (non-TNM)
        items cardinality matches {1..*; unordered} matches {
            ELEMENT[id2] occurrences matches {0..1} matches {    -- Dukes Score
                value matches {
                    DV_CODED_TEXT[id9003] matches {
                        defining_code matches {[ac9000]}    -- Dukes Score (synthesised)
                    }
                }
            }
            ELEMENT[id3] occurrences matches {0..1} matches {    -- ACPS Score
                value matches {
                    DV_CODED_TEXT[id9004] matches {
                        defining_code matches {[ac9001]}    -- ACPS Score (synthesised)
                    }
                }
            }
            ELEMENT[id17] occurrences matches {0..1} matches {    -- ACPS Concord variant
                value matches {
                    DV_CODED_TEXT[id9005] matches {
                        defining_code matches {[ac9002]}    -- ACPS Concord variant (synthesised)
                    }
                }
            }
        }
    }

