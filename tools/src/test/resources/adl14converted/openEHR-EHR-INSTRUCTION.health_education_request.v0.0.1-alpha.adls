                            }
                            ELEMENT[id5] matches {    -- Details
                                value matches {
                                    DV_TEXT[id9001] 
                                }
                            }
                            ELEMENT[id6] occurrences matches {0..1} matches {    -- Number of sessions
                                value matches {
                                    DV_COUNT[id9002] matches {
                                        magnitude matches {|>=1|}
                                    }
                                }
                            }
                            ELEMENT[id7] occurrences matches {0..1} matches {    -- Reason
                                value matches {
                                    DV_TEXT[id9003] 
                                }
                            }
                            ELEMENT[id8] occurrences matches {0..1} matches {    -- Proposed outcome
                                value matches {
                                    DV_TEXT[id9004] 
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id9] matches {
                items cardinality matches {0..*; unordered} matches {
                    allow_archetype CLUSTER[id10] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

