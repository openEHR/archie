definition
    OBSERVATION[id1] matches {    -- Harris hip score
        data matches {
            HISTORY[id2] matches {
                events cardinality matches {1..*; unordered} matches {
                    EVENT[id3] matches {    -- Any event
                        data matches {
                            ITEM_TREE[id4] matches {
                                items cardinality matches {0..*; unordered} matches {
                                    ELEMENT[id5] occurrences matches {0..1} matches {    -- Harris hip score result
                                        value matches {
                                            DV_QUANTITY[id9002] matches {
                                                property matches {[at9000]}    -- Qualified real
                                                magnitude matches {|0.0..100.0|}
                                                units matches {"1"}
                                                precision matches {2}
                                            }
                                        }
                                    }
                                    ELEMENT[id6] occurrences matches {0..1} matches {    -- Grading
                                        value matches {
                                            DV_CODED_TEXT[id9003] matches {
                                                defining_code matches {[ac9001]}    -- Grading (synthesised)
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id11] matches {
                items cardinality matches {0..*; unordered} matches {
                    allow_archetype CLUSTER[id12] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

