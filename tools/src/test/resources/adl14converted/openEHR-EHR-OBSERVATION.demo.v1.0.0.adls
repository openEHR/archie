            ["ac9054"] = <
                text = <"Multimedia (synthesised)">
                description = <"Multimedia-datatyper brukes for å registrere en av flere mulige typer multimediafiler. I dette eksempelet er alle mulige typer eksplisitt valgte. (synthesised)">
            >
            ["at9055"] = <
