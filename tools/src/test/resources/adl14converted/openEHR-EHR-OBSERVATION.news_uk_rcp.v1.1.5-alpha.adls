            >
            ["at24"] = <
                text = <"A">
                description = <"Alert och vaken. Sätts till 0 poäng.">
            >
            ["at23"] = <
                text = <"35,1-36,0 eller 38,1-39,0">
                description = <"Kroppstemperatur mellan 35,1 och 36,0 grader Celsius alternativt 38,1 och 39,0 grader Celsius. Sätts till 0 poäng.">
            >
            ["at22"] = <
                text = <"36,1-38,0">
