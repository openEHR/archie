            >
            ["at14"] = <
                text = <"111-219">
                description = <"Systoliskt blodtryck mellan 111 och 219 mmHg. Sätts till 0 poäng.">
            >
            ["at13"] = <
                text = <"51-90">
                description = <"Hjärtfrekvens mellan 51 och 90 slag per minut. Sätts till 0 poäng.">
