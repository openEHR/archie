                        }
                    }
                    ELEMENT[id92] occurrences matches {0..1} matches {    -- Start level
                        value matches {
                            DV_QUANTITY[id9039] matches {
                                property matches {[at9002]}    -- Loudness
                                units matches {"dB"}
