    details = <
        ["ar-sy"] = <
            language = <[ISO_639-1::ar-sy]>
            purpose = <"لتسجيل تفاصيل نتيجة اختبار المُخَطَّط الطبلي">
            keywords = <"المخطط الطبلي", "قياس الطبل
    
