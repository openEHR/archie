definition
    CLUSTER[id1] occurrences matches {0..1} matches {    -- Person additional demographic data
        items matches {
            ELEMENT[id2] matches {    -- Sex
                value matches {
                    DV_CODED_TEXT[id9001] matches {
                        defining_code matches {[ac9000]}    -- Sex (synthesised)
                    }
                }
            }
            ELEMENT[id3] matches {    -- Mother's family name
                value matches {
                    DV_TEXT[id9002] 
                }
            }
            ELEMENT[id4] matches {    -- Identification comment
                value matches {
                    DV_TEXT[id9003] 
                }
            }
        }
    }

