definition
    OBSERVATION[id1] matches {    -- Auditory brainstem response (ABR) result
        data matches {
            HISTORY[id2] matches {
                events cardinality matches {1..*; unordered} matches {
                    POINT_EVENT[id3] occurrences matches {0..1} matches {    -- Any event
