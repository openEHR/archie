definition
    CLUSTER[id1] matches {    -- Identifier
        items cardinality matches {1..*; unordered} matches {
            ELEMENT[id2] occurrences matches {0..1} matches {    -- Value
                value matches {
                    DV_IDENTIFIER[id9001]
                }
            }
            ELEMENT[id3] occurrences matches {0..1} matches {    -- Use
                value matches {
                    DV_CODED_TEXT[id9002] matches {
                        defining_code matches {[ac9000]}    -- Use (synthesised)
                    }
                }
            }
            ELEMENT[id8] occurrences matches {0..1} matches {    -- Valid period start
                value matches {
                    DV_DATE_TIME[id9003]
                }
            }
            ELEMENT[id9] occurrences matches {0..1} matches {    -- Valid period end
                value matches {
                    DV_DATE_TIME[id9004]
                }
            }
        }
    }

