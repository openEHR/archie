                            DV_TEXT[id9028]
                        }
                    }
                    ELEMENT[id15] occurrences matches {0..1} matches {    -- Service type
                        value matches {
                            DV_TEXT[id9029]
                        }
                    }
                    ELEMENT[id14] occurrences matches {0..1} matches {    -- Description
