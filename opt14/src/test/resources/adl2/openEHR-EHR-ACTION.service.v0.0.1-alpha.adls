                        include
                            archetype_id/value matches {/.*/}
                    }
                    ELEMENT[id19] occurrences matches {0..1} matches {    -- Receiver identifier
                        value matches {
                            DV_IDENTIFIER[id9036]
                        }
                    }
                    allow_archetype CLUSTER[id20] matches {     -- Receiver
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

